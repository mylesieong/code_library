/* CSS Document */

body {
	background: #fff;
}

div.SystemName{
 font-size: 30;
 font-family: 'Arial Black', Gadget, sans-serif;
 color: #013652;
 background-color: #FFFFFF;
 text-align: left;
 line-height: 1.3em;
 font-style: italic;
}

td, .text {
 font-size: 12px;
 font-family: Arial, Helvetica, sans-serif;
 color: #272727;
 background-color: #FFFFFF;
 text-align: left;
 line-height: 1.3em;
 font-style: inherit;
}

input.inputbox{
COLOR: #616161; FONT-SIZE: 12px; width:95%;
}

input.key{
COLOR: #616161; FONT-SIZE: 12px; width:100%;
}

input.password{
COLOR: #616161; FONT-SIZE: 12px; width:95%;
}

input.btn{
border:  #000 1px solid;
Font-size: 12px;
COLOR: #000000;
FONT-FAMILY: Verdana;
TEXT-DECORATION: none;
background-color:#F0FFF0;
margin-top: 18px;
right:30px;
}

.option{
Font-size: 12px;
FONT-FAMILY: Verdana;
TEXT-DECORATION: none;
width:200px;
COLOR: #616161;
}

.ContainerFrame {
	BORDER-BOTTOM: rgb(97,97,97) 2px solid;
	BORDER-LEFT: rgb(97,97,97) 2px solid;
	PADDING: 0px;
	padding-top:0px; 
	BACKGROUND-COLOR: #30974e;
	MARGIN: 0px;
	FONT-FAMILY: Arial,Helvetica,Sans-Serif;
	COLOR: #a6dcb5;
	FONT-SIZE: 12px;
	BORDER-TOP: rgb(97,97,97) 2px solid;
	FONT-WEIGHT: bold;
	BORDER-RIGHT: rgb(97,97,97) 2px solid;
	width:300px;
}

.ContainerFrameHeader {
BORDER-BOTTOM: 0px;
TEXT-ALIGN: left;
BORDER-LEFT: 0px;
PADDING-BOTTOM: 0px;
BACKGROUND-COLOR: #30974e;
PADDING-LEFT: 0px;
PADDING-RIGHT: 0px;
COLOR: white;
FONT-SIZE: 12px;
BORDER-TOP: 0px;
FONT-WEIGHT: bold;
BORDER-RIGHT: 0px;
PADDING-TOP: 0px;
}

.Labels {
	COLOR: #616161; FONT-SIZE: 12px; PADDING-TOP: 3px; align: left; FONT-FAMILY: Arial,helvetica,Sans-Serif;FONT-WEIGHT: normal;
}

.LinkButton {
	PADDING-BOTTOM: 1px; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: #616161; FONT-SIZE: 12px; CURSOR: hand; TEXT-DECORATION: none; PADDING-TOP: 3px;
}
.LinkButtonBackground {
	PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #dae6d0; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: #006666; FONT-SIZE: 12px; CURSOR: hand; FONT-WEIGHT: bold; TEXT-DECORATION: none; PADDING-TOP: 1px
}
LinkButtonBackground:link {
	PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #dae6d0; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: #006666; FONT-SIZE: 12px; CURSOR: hand; FONT-WEIGHT: bold; TEXT-DECORATION: none; PADDING-TOP: 1px
}
LinkButtonBackground:visited {
	PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #dae6d0; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: #006666; FONT-SIZE: 12px; CURSOR: hand; FONT-WEIGHT: bold; TEXT-DECORATION: none; PADDING-TOP: 1px
}
LinkButtonBackground:active {
	PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #dae6d0; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: #006666; FONT-SIZE: 12px; CURSOR: hand; FONT-WEIGHT: bold; TEXT-DECORATION: none; PADDING-TOP: 1px
}
.LinkButtonBackground:hover {
	PADDING-BOTTOM: 1px; BACKGROUND-COLOR: #dae6d0; PADDING-LEFT: 3px; PADDING-RIGHT: 3px; COLOR: orange; FONT-SIZE: 12px; CURSOR: hand; FONT-WEIGHT: bold; TEXT-DECORATION: none; PADDING-TOP: 1px
}

.invisible {
	display:none;
}

.errormsg{
	color: #D8000C;  
	 font-family: 'Arial', Gadget, sans-serif;
}

img{
	border-style: none;
}

table.result,table.error {
	border-collapse: collapse;
	margin: 24px;
	font-size: 1.1em;
}

th.result {
	background: #006400;
	color: #fff;
	font-weight: bold;
	padding: 10px 2px 4px 8px;
	text-align: left;
	border-right: 1px solid #fff;
	line-height: 1.2;
	font-size:12px;
	font-family: Arial, Helvetica, sans-serif;
}

td.result,td.error{
	color: #616161;
	padding: 6px 2px 6px 10px;
	border-bottom: 2px solid #95bce2;
	vertical-align: top;
	font-size:12px;
	font-family: Arial, Helvetica, sans-serif;
	text-align: left;
	line-height: 1em;
	width: auto;
}

tr.error td.error {
	background: #FFFFDF;
	width: 600px;
}

tr.result:hover td.result {
	background: #eaf2f9;
}

tr.error.over td.error, tr.error:hover td.error {
	background: #DEFFAC;
}

.msgError {
	align:center;
   background: #FFE7E7;
   padding: 15px 15px 15px 15px;
   border-top: 2px solid #E77D7B;
   border-bottom: 2px solid #E77D7B;
   border:2px solid #E77D7B;
   font-size:12px;
   font-family:Verdana;
   width: 200px;
}

.msgErrorDiv {
	align:center;
   background: #FFE7E7;
   border: 0;
   font-size:12px;
   font-family:Verdana;
   width: 200px;
}

html {
scrollbar-base-color: #30974e;
scrollbar-arrow-color: white;
scrollbar-track-color: #ccffcc;

scrollbar-shadow-color: black;
scrollbar-lightshadow-color: black; 
scrollbar-darkshadow-color: gray;

scrollbar-highlight-color: white;
scrollbar-3dlight-color: black;
}

a:link, td.result a:link {color: gray}
a:visited, td.result a:visited {color: black}
a:active, td.result a:active {color: green}
a:hover, td.result a:hover{color: gray}